<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.todoserver.mapper.TodoMapper">
    <select id="getTime" resultType="java.lang.String">
        select now()
    </select>

    <select id="todoList" resultType="com.example.todoserver.domain.Todo">
        select * from todoSB
    </select>

    <insert id="todoInput">
        insert into todoSB (title, memo) values (#{title}, #{memo})
    </insert>
</mapper>